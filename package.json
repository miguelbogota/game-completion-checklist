{
  "name": "game-completion-checklist",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "npm": "9",
    "node": "18"
  },
  "scripts": {
    "prepare": "husky install",
    "watch": "next dev -p 4000",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test:unit": "vitest run .",
    "test:unit-watch": "vitest .",
    "test:spelling": "cspell stdin --no-must-find-files --file-list",
    "test:lint-staged": "lint-staged",
    "generate:schema": "node scripts/generate-schema.js"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.13.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lodash": "^4.17.21",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "zustand": "^4.4.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.199",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vanilla-extract/next-plugin": "^2.3.1",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "cspell": "^7.3.7",
    "csstype": "^3.1.2",
    "eslint": "^8.50.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globals": "^13.22.0",
    "husky": "^8.0.3",
    "jest-extended": "^4.0.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "ts-json-schema-generator": "^1.3.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
